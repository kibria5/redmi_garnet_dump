<!--
    Input:
        device/xiaomi/missi/device_framework_compatibility_matrix/mrm/vendor_framework_compatibility_matrix.xml
        device/xiaomi/missi/device_framework_compatibility_matrix/mikeybag/vendor_framework_compatibility_matrix.xml
        device/xiaomi/missi/device_framework_compatibility_matrix/micrtk/vendor_framework_compatibility_matrix.xml
        device/xiaomi/missi/device_framework_compatibility_matrix/misecmedia/vendor_framework_compatibility_matrix.xml
        device/xiaomi/missi/device_framework_compatibility_matrix/mseccam/vendor_framework_compatibility_matrix.xml
-->
<compatibility-matrix version="8.0" type="framework">
    <hal format="aidl" optional="true">
        <name>vendor.xiaomi.hardware.micrtk</name>
        <interface>
            <name>IMiCertKeyService</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="aidl" optional="true">
        <name>vendor.xiaomi.hardware.mikeybag</name>
        <interface>
            <name>IMiKeyBagService</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="aidl" optional="true">
        <name>vendor.xiaomi.hardware.misauth</name>
        <interface>
            <name>IMiSharedAuth</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="aidl" optional="true">
        <name>vendor.xiaomi.hardware.misecmedia</name>
        <interface>
            <name>IMiSecMediaService</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="aidl" optional="true">
        <name>vendor.xiaomi.hardware.mrm</name>
        <interface>
            <name>IMrm</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="aidl" optional="true">
        <name>vendor.xiaomi.hardware.mrm</name>
        <interface>
            <name>IMiCertificate</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="aidl" optional="true">
        <name>vendor.xiaomi.hardware.mrm</name>
        <interface>
            <name>IMrmRemoteAuth</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="aidl" optional="true">
        <name>vendor.xiaomi.hardware.seccam</name>
        <interface>
            <name>IMiSecCam</name>
            <instance>default</instance>
        </interface>
    </hal>
</compatibility-matrix>
